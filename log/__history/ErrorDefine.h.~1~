#pragma once
#ifndef ERROR_DEFINE_H
#define ERROR_DEFINE_H

#define WARN_DATA_LOAD_FAIL     33  // Eroare la încărcarea datelor
#define WARN_SQL_ERROR          22  // Eroare în execuția SQL
#define WARN_NO_SELECTED_ROW    44  // Nu există rând selectat pentru ștergere
#define WARN_DELETE_FAILED      55  // Eroare la ștergerea datelor
#define WARN_SLOW_RESPONSE      66  // Serverul răspunde lent
#define WARN_LOW_RESOURCES      77  // Resurse insuficiente

#endif // ERROR_DEFINE_H